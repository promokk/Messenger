# platform :ios, '10.0'

target 'Weather' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Weather
  pod 'Alamofire'
  pod 'SwiftyJSON', '~> 4.2.0'
  pod 'Kingfisher'
  pod 'RealmSwift'
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'

end
